{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"oneOf": [
		{
			"type": "null"
		},
		{
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"access_token": {
					"type": "string"
				},
				"refresh_token": {
					"type": "string"
				},
				"expires_in": {
					"type": "number"
				},
				"scope": {
					"type": "array",
					"items": {
						"type": "string"
					}
				}
			},
			"required": [
				"access_token",
				"refresh_token",
				"expires_in",
				"scope"
			]
		}
	]
}
